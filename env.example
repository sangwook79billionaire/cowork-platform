# Firebase 설정
NEXT_PUBLIC_FIREBASE_API_KEY=your_firebase_api_key
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
NEXT_PUBLIC_FIREBASE_PROJECT_ID=your_project_id
NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
NEXT_PUBLIC_FIREBASE_APP_ID=your_app_id

# Google Gemini API 설정
GEMINI_API_KEY=your_gemini_api_key

# Google OAuth 설정
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
GOOGLE_REDIRECT_URI=http://localhost:3000/api/google/auth/callback

# Naver News API 설정 (실제 뉴스 검색용)
NAVER_CLIENT_ID=your_naver_client_id
NAVER_CLIENT_SECRET=your_naver_client_secret

# 뉴스 API 설정 (선택사항 - 현재는 모의 데이터 사용)
# NEWS_API_KEY=your_news_api_key

# 예시:
# NEXT_PUBLIC_FIREBASE_API_KEY=AIzaSyC...
# NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN=cowork-platform.firebaseapp.com
# NEXT_PUBLIC_FIREBASE_PROJECT_ID=cowork-platform
# NEXT_PUBLIC_FIREBASE_STORAGE_BUCKET=cowork-platform.appspot.com
# NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=123456789
# NEXT_PUBLIC_FIREBASE_APP_ID=1:123456789:web:abcdef
# GEMINI_API_KEY=AIzaSyC...
# GOOGLE_CLIENT_ID=123456789-abcdef.apps.googleusercontent.com
# GOOGLE_CLIENT_SECRET=GOCSPX-abcdefghijklmnop
# GOOGLE_REDIRECT_URI=http://localhost:3000/api/google/auth/callback
# NEWS_API_KEY=your_news_api_key_here (선택사항) 